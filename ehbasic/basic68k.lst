F00:0091       *************************************************************************************
F00:0092       *
F00:0093       * the following code is simulator specific, change to suit your system
F00:0094       
F00:0095       * Output character to the console from register d0.b
F00:0096       
F00:0097       VEC_OUT
F00:0098               MOVEM.L  A0/D1,-(A7)    * Save working registers
               S02:0000C002:  48 E7 40 80
F00:0099               LEA.L    ACIA_1,A0      * A0 points to console ACIA
               S02:0000C006:  41 F9 00 01 00 40
F00:0100       TXNOTREADY
F00:0101               MOVE.B   (A0),D1        * Read ACIA status
               S02:0000C00C:  12 10
F00:0102               BTST     #1,D1          * Test TDRE bit
               S02:0000C00E:  08 01 00 01
F00:0103               BEQ.S    TXNOTREADY     * Until ACIA Tx ready
               S02:0000C012:  67 F8
F00:0104               MOVE.B   D0,2(A0)       * Write character to send
               S02:0000C014:  11 40 00 02
F00:0105               MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:0000C018:  4C DF 01 02
F00:0106               RTS
               S02:0000C01C:  4E 75
F00:0107       
F00:0108       * Output character to the second (aux) serial port from register d0.b
F00:0109       
F00:0110        ifne   FLASH_SUPPORT
F00:0111       
F00:0112       VEC_OUT2
F00:0113               MOVEM.L  A0/D1,-(A7)    * Save working registers
F00:0114               LEA.L    ACIA_2,A0      * A0 points to console ACIA
F00:0115       TXNOTREADY1
F00:0116               MOVE.B   (A0),D1        * Read ACIA status
F00:0117               BTST     #1,D1          * Test TDRE bit
F00:0118               BEQ.s    TXNOTREADY1    * Until ACIA Tx ready
F00:0119               MOVE.B   D0,2(A0)       * Write character to send
F00:0120               MOVEM.L  (A7)+,A0/D1    * Restore working registers
F00:0121               RTS
F00:0122       
F00:0123       * Output null terminated string pointed to by A0 to first serial port.
F00:0124       
F00:0125       PRINTSTRING1
F00:0126               MOVEM.L  A0/D0,-(A7)    * Save working registers
F00:0127       LP1     CMP.B    #0,(A0)        * Is it null?
F00:0128               BEQ      RET1           * If so, return
F00:0129               MOVE.B   (A0)+,D0       * Get character and advance pointer
F00:0130               JSR      VEC_OUT        * Output it
F00:0131               BRA      LP1            * Continue for rest of string
F00:0132       
F00:0133       RET1    MOVEM.L  (A7)+,A0/D0    * Restore working registers
F00:0134               RTS                     * Return
F00:0135       
F00:0136       * Output null terminated string pointed to by A0 to second serial port.
F00:0137       
F00:0138       PRINTSTRING2
F00:0139               MOVEM.L  A0/D0,-(A7)    * Save working registers
F00:0140       LP2     CMP.B    #0,(A0)        * Is it null?
F00:0141               BEQ      RET2           * If so, return
F00:0142               MOVE.B   (A0)+,D0       * Get character and advance pointer
F00:0143               JSR      VEC_OUT2       * Output it
F00:0144               BRA      LP2            * Continue for rest of string
F00:0145       
F00:0146       RET2    MOVEM.L  (A7)+,A0/D0    * Restore working registers
F00:0147               RTS                     * Return
F00:0148       
F00:0149        endc
