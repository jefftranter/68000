F00:0151       *************************************************************************************
F00:0152       *
F00:0153       * input a character from the console into register d0
F00:0154       * else return Cb=0 if there's no character available
F00:0155       
F00:0156       VEC_IN
F00:0157               MOVEM.L  A0/D1,-(A7)    * Save working registers
               S02:0000C01E:  48 E7 40 80
F00:0158               LEA.L    ACIA_1,A0      * A0 points to console ACIA
               S02:0000C022:  41 F9 00 01 00 40
F00:0159               MOVE.B   (A0),D1        * Read ACIA status
               S02:0000C028:  12 10
F00:0160               BTST     #0,D1          * Test RDRF bit
               S02:0000C02A:  08 01 00 00
F00:0161               BEQ.S    RXNOTREADY     * Branch if ACIA Rx not ready
               S02:0000C02E:  67 0E
F00:0162               MOVE.B   2(A0),D0       * Read character received
               S02:0000C030:  10 28 00 02
F00:0163               MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:0000C034:  4C DF 01 02
F00:0164               ORI.B    #1,CCR         * Set the carry, flag we got a byte
               S02:0000C038:  00 3C 00 01
F00:0165               RTS                     * Return
               S02:0000C03C:  4E 75
F00:0166       RXNOTREADY
F00:0167               MOVEM.L  (A7)+,A0/D1    * Restore working registers
               S02:0000C03E:  4C DF 01 02
F00:0168               ANDI.B   #$FE,CCR       * Clear the carry, flag character not available
               S02:0000C042:  02 3C 00 FE
F00:0169               RTS
               S02:0000C046:  4E 75
F00:0170       
F00:0171       * Input routine used in LOAD mode to read file from USB flash storage.
F00:0172       
F00:0173        ifne   FLASH_SUPPORT
F00:0174       
F00:0175       VEC_IN2
F00:0176               MOVEM.L  A0/D1,-(A7)    * Save working registers
F00:0177               LEA.L    VEC_OUT2,A0    * Redirect output to aux. port.
F00:0178               MOVE.L   A0,V_OUTPv(a3)
F00:0179       
F00:0180       * The first time, send READ <filename> 1 1
F00:0181       * Subsequent times, send READ <filename> n 1
F00:0182       
F00:0183               LEA      LAB_READN(pc),A0 * Send READ command string
F00:0184               BSR      PRINTSTRING2   * Print null terminated string
F00:0185       
F00:0186               LEA      load_filename(A3),A0 * Send filename string
F00:0187               BSR      PRINTSTRING2   * Print null terminated string
F00:0188       
F00:0189               MOVE.B   #' ',D0       * Send space
F00:0190               JSR      VEC_OUT2
F00:0191       
F00:0192               CMP.B    #1,load_first(A3) * First time?
F00:0193               BNE      NOTFIRST1
F00:0194               MOVE.B   #'1',D0        * Send '1'
F00:0195               CLR.B    load_first(A3) * Clear first flag
F00:0196               BRA      SENDCMD1
F00:0197       NOTFIRST1
F00:0198               MOVE.B   #'n',D0        * Send 'n'
F00:0199       SENDCMD1
F00:0200               JSR      VEC_OUT2
F00:0201               MOVE.B   #' ',D0        * Send space
F00:0202               JSR      VEC_OUT2
F00:0203               MOVE.B   #'1',D0        * Send '1'
F00:0204               JSR      VEC_OUT2
F00:0205               MOVE.B   #$0D,D0        * Send <Return>
F00:0206               JSR      VEC_OUT2
F00:0207       
F00:0208               LEA.L    VEC_OUT,A0     * Redirect output back to console port.
F00:0209               MOVE.L   A0,V_OUTPv(a3)
F00:0210       
F00:0211       * Read one byte from USB host
F00:0212       
F00:0213               LEA.L    ACIA_2,A0      * A0 points to console ACIA
F00:0214       RXNOTREADY2
F00:0215               MOVE.B   (A0),D1        * Read ACIA status
F00:0216               BTST     #0,D1          * Test RDRF bit
F00:0217               BEQ.S    RXNOTREADY2    * Branch if ACIA Rx not ready
