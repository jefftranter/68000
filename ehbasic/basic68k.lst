F00:9229       *************************************************************************************
F00:9230       * EhBASIC keywords quick reference list								*
F00:9231       *************************************************************************************
F00:9232       
F00:9233       * glossary
F00:9234       
F00:9235       *		<.>		  required
F00:9236       *		{.|.}		  one of required
F00:9237       *		[.]		  optional
F00:9238       *		...		  may repeat as last
F00:9239       
F00:9240       *		any		= anything
F00:9241       *		num		= number
F00:9242       *		state		= statement
F00:9243       *		n		= positive integer
F00:9244       *		str		= string
F00:9245       *		var		= variable
F00:9246       *		nvar		= numeric variable
F00:9247       *		svar		= string variable
F00:9248       *		expr		= expression
F00:9249       *		nexpr		= numeric expression
F00:9250       *		sexpr		= string expression
F00:9251       
F00:9252       * statement separator
F00:9253       
F00:9254       * :		. [<state>] : [<state>]						* done
F00:9255       
F00:9256       * number bases
F00:9257       
F00:9258       * %		. %<binary num>							* done
F00:9259       * $		. $<hex num>							* done
F00:9260       
F00:9261       * commands
F00:9262       
F00:9263       * END		. END									* done
F00:9264       * FOR		. FOR <nvar>=<nexpr> TO <nexpr> [STEP <nexpr>]		* done
F00:9265       * NEXT	. NEXT [<nvar>[,<nvar>]...]					* done
F00:9266       * DATA	. DATA [{num|["]str["]}[,{num|["]str["]}]...]		* done
F00:9267       * INPUT	. INPUT [<">str<">;] <var>[,<var>[,<var>]...]		* done
F00:9268       * DIM		. DIM <var>(<nexpr>[,<nexpr>[,<nexpr>]])			* done
F00:9269       * READ	. READ <var>[,<var>[,<var>]...]				* done
F00:9270       * LET		. [LET] <var>=<expr>						* done
F00:9271       * DEC		. DEC <nvar>[,<nvar>[,<nvar>]...]				* done
F00:9272       * GOTO	. GOTO <n>								* done
F00:9273       * RUN		. RUN [<n>]								* done
F00:9274       * IF		. IF <expr>{GOTO<n>|THEN<{n|comm}>}[ELSE <{n|comm}>]	* done
F00:9275       * RESTORE	. RESTORE [<n>]							* done
F00:9276       * GOSUB	. GOSUB <n>								* done
F00:9277       * RETURN	. RETURN								* done
F00:9278       * REM		. REM [<any>]							* done
F00:9279       * STOP	. STOP								* done
F00:9280       * ON		. ON <nexpr>{GOTO|GOSUB}<n>[,<n>[,<n>]...]		* done
F00:9281       * NULL	. NULL <nexpr>							* done
F00:9282       * INC		. INC <nvar>[,<nvar>[,<nvar>]...]				* done
